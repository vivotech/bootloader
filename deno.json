{
  "name": "@artery/bootloader",
  "exports": {
    "./inspect": "./inspect.ts",
    ".": "./main.ts"
  }, 
  "version": "0.1.0",
  "tasks": {
    "dev": "deno run --watch main.ts",
    "inspect": "deno run --allow-read --allow-run --allow-write inspect.ts",
    "main": "deno run --allow-run --allow-read --allow-write main.ts",
    "compile": "deno compile --allow-run --allow-net main.ts"
  },
  "license": "GPL-3.0",
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/cli": "jsr:@std/cli@^1.0.9",
    "@std/path": "jsr:@std/path@^1.0.8"
  }
}
